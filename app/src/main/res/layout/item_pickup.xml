<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

    </data>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5sp"
        android:layout_marginStart="5sp"
        android:layout_marginEnd="5sp"
        android:backgroundTint="@color/white">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/layout_box_stroke_recycler">
            <LinearLayout
                android:id="@+id/pickupList"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingTop="10sp"
                android:paddingBottom="10sp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/date_pickupList_washer"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="10sp"
                                android:textColor="@color/dgray"
                                android:text="2022.02.02"
                                android:textSize="16sp"
                                android:textStyle="bold" />
                            <TextView
                                android:id="@+id/time_pickupList_washer"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="26sp"
                                android:layout_weight="1"
                                android:textColor="@color/dgray"
                                android:text="지금당장"
                                android:textSize="16sp"
                                android:textStyle="bold" />
                            <TextView
                                android:id="@+id/name_pickupManager"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="10sp"
                                android:textColor="@color/aqua"
                                android:text="8,000원"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/wash_type_orderList_washer"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/dgray"
                                android:layout_marginStart="10sp"
                                android:layout_marginTop="5sp"
                                android:text="내부+외부세차(외제차)"
                                android:textSize="14sp"
                                android:textStyle="bold" />
                            <TextView
                                android:id="@+id/car_number_orderList_washer"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/dgray"
                                android:layout_marginTop="5sp"
                                android:layout_marginStart="10sp"
                                android:text="000허0000"
                                android:textSize="14sp"
                                android:textStyle="bold" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/brand_orderList_washer"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="10sp"
                                android:layout_marginTop="5sp"
                                android:text="벤츠"
                                android:textColor="@color/dgray"
                                android:textSize="14sp"
                                android:textStyle="bold"/>

                            <TextView
                                android:id="@+id/styleName_orderList_washer"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="5sp"
                                android:layout_marginTop="5sp"
                                android:text="GLC220"
                                android:textColor="@color/dgray"
                                android:textSize="14sp"
                                android:textStyle="bold"/>
                            <TextView
                                android:id="@+id/car_kinds_orderList_washer"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/dgray"
                                android:layout_marginTop="5sp"
                                android:layout_marginStart="5sp"
                                android:text="SUV"
                                android:textSize="14sp"
                                android:textStyle="bold"/>
                            <TextView
                                android:id="@+id/car_color_orderList_washer"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/dgray"
                                android:layout_marginTop="5sp"
                                android:layout_marginStart="5sp"
                                android:text="BLACK"
                                android:textSize="14sp"
                                android:textStyle="bold"/>

                            <TextView
                                android:id="@+id/car_size_orderList_washer"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/dgray"
                                android:layout_marginTop="5sp"
                                android:layout_marginStart="5sp"
                                android:text="준중형차"
                                android:textSize="14sp"
                                android:textStyle="bold"/>
                        </LinearLayout>
                        <Space
                            android:layout_width="match_parent"
                            android:layout_height="5sp">

                        </Space>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:gravity="center_vertical">
                                <TextView
                                    android:layout_width="75sp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10sp"
                                    android:text="픽업/탁송지"
                                    android:textColor="@color/dgray"
                                    android:textStyle="bold"/>
                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/arrow_right_alt"/>
                                <TextView
                                    android:id="@+id/owner_address_orderList_washer"
                                    android:layout_width="match_parent"
                                    android:layout_height="50sp"
                                    android:layout_marginStart="5sp"
                                    android:layout_marginEnd="10sp"
                                    android:padding="2sp"
                                    android:gravity="center_vertical"
                                    android:maxLines="2"
                                    android:ellipsize="end"
                                    android:text="서울특별시 논현동 111-111 1층 논현1동 논현세차장"
                                    android:textColor="@color/dgray"
                                    android:textSize="14sp" />
                            </LinearLayout>
                            <Space
                                android:layout_width="match_parent"
                                android:layout_height="5sp">
                            </Space>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:gravity="center_vertical">
                                <TextView
                                    android:layout_width="75sp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10sp"
                                    android:text="차량 입고지"
                                    android:textColor="@color/dgray"
                                    android:textStyle="bold"/>
                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/arrow_right_alt"/>
                                <TextView
                                    android:id="@+id/washer_address_orderList_washer"
                                    android:layout_width="match_parent"
                                    android:layout_height="50sp"
                                    android:layout_marginStart="5sp"
                                    android:layout_marginEnd="10sp"
                                    android:padding="2sp"
                                    android:gravity="center_vertical"
                                    android:maxLines="2"
                                    android:ellipsize="end"
                                    android:text="서울특별시 논현동 111-111 1층 논현1동 논현세차장"
                                    android:textColor="@color/dgray"
                                    android:textSize="14sp" />
                            </LinearLayout>

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:gravity="center_vertical">

                            <com.google.android.material.chip.Chip
                                android:id="@+id/cfmOrder_pickupManager"
                                android:layout_width="wrap_content"
                                android:layout_height="50dp"
                                android:layout_marginStart="10sp"
                                android:textColor="@color/white"
                                android:text="픽업하러 가기"
                                android:backgroundTint="@color/aqua"
                                android:textSize="16sp"
                                android:textStyle="bold" />
                            <Space
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="35sp"
                                android:drawableStart="@drawable/recycler_bubble"
                                android:paddingStart="10sp"
                                android:paddingEnd="16sp"
                                android:background="@drawable/recyclerview_radius_washer_pickup"
                                android:gravity="center"
                                android:text="픽업대기"
                                android:textSize="16sp"
                                android:textColor="@color/white"
                                android:textStyle="bold"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>