                            android:textSize="14sp" />

                        <TextView
                            android:layout_width="90sp"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:text="광택"
                            android:textSize="14sp" />

                        <TextView
                            android:id="@+id/polish_cost"
                            android:layout_width="90sp"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:text="20000"
                            android:textSize="14sp" />
                    </LinearLayout>
                </LinearLayout>

                <Space
                    android:layout_width="match_parent"
                    android:layout_height="5sp"></Space>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="90sp"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:text="내부+외부"
                            android:textSize="14sp" />

                        <TextView
                            android:id="@+id/inOutside_time"
                            android:layout_width="90sp"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:text="30분"
                            android:textSize="14sp" />

                        <TextView
                            android:layout_width="90sp"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal" />

                        <TextView
                            android:layout_width="90sp"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal" />
                    </LinearLayout>
                </LinearLayout>

                <Space
                    android:layout_width="match_parent"
                    android:layout_height="25sp"></Space>
            </LinearLayout>
        </androidx.cardview.widget.CardView>
    </FrameLayout>
</layout>